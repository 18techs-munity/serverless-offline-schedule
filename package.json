    "@types/node": "^12.7.1",
    "@types/sinon": "^7.0.13",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "concurrently": "^4.1.1",
    "coveralls": "^3.0.6",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "sinon": "^7.4.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  }
